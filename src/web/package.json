{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/xdrop-web",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://xdrop.polymedia.app",
    "description": "Cross-chain airdrop system on Sui",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-xdrop"
    },
    "keywords": [],
    "scripts": {
        "build": "vite build",
        "clean": "rm -rf dist/ node_modules/ .turbo/ .wrangler/",
        "deploy-dev": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-xdrop --branch dev",
        "deploy-test": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-xdrop --branch test",
        "deploy-prod": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-xdrop --branch main",
        "dev": "vite",
        "lint": "eslint src/ --report-unused-disable-directives --max-warnings 0",
        "preview": "pnpm build && vite preview",
        "typecheck": "tsc -b"
    },
    "dependencies": {
        "@mysten/dapp-kit": "^0.14.30",
        "@mysten/sui": "^1.14.2",
        "@polymedia/coinmeta-react": "^0.0.17",
        "@polymedia/suitcase-core": "^0.0.35",
        "@polymedia/suitcase-react": "^0.0.35",
        "@polymedia/xdrop-sdk": "workspace:*",
        "@tanstack/react-query": "^5.59.20",
        "ethers": "^6.13.4",
        "normalize.css": "^8.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.28.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react-swc": "^3.7.1",
        "less": "^4.2.0",
        "typescript": "^5.6.3",
        "vite": "^5.4.11",
        "wrangler": "^3.86.1"
    },
    "type": "module",
    "sideEffects": false
}
